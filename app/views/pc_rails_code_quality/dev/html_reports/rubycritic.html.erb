<div class="code-quality-container">
  <%= link_to 'Re-run', dev_run_rubycritic_html_report_path, class: 'code-quality-button' %>
  <% if File.exists?(Rails.root.join("public/reports/ruby_critic/overview.html")) %>
    <iframe height="100%" id="rubycritic-html-report" name="<%= Time.now %>" src="/reports/ruby_critic/overview.html?pp=disable" width="100%"></iframe>
  <% else %>
    <div class="no-report">
      No report yet
    </div>
  <% end %>
</div>

